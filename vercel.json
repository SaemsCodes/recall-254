{
  "redirects": [
    { "source": "/IEBC.office", "destination": "/iebc-office", "permanent": true },
    { "source": "/iebc", "destination": "/iebc-office", "permanent": true },
    { "source": "/iebc-", "destination": "/iebc-office", "permanent": true },
    { "source": "/IEBC-OFFICE", "destination": "/iebc-office", "permanent": true },
    { "source": "/iebce-office", "destination": "/iebc-office", "permanent": true },
    { "source": "/iebcoffice", "destination": "/iebc-office", "permanent": true }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.css",
      "headers": [{ "key": "Content-Type", "value": "text/css" }]
    },
    {
      "source": "/(.*)\\.js", 
      "headers": [{ "key": "Content-Type", "value": "application/javascript" }]
    }
  ]
}
